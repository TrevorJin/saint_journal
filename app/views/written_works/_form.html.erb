<%= simple_form_for(@written_work) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :publication_date %>
    <%= f.input :description %>
    <%= f.input :publication_accuracy %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
